}

.textbox{
  display: flex;
  flex-direction: column;
  justify-content: center;
  }

.huwa{
	opacity: 0;
	transform: translateY(30px);
	transition: opacity 1.5s, transform 1s;
}
.huwa.active {
	opacity: 1;
	transform: translateY(0px);
}



